{
    "editor.fontFamily": "'Fira Code', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback', 'Font Awesome 5 Free Solid', 'Font Awesome 5 Free Regular','Font Awesome 5 Brands Regular'",
    "editor.rulers": [
        80, 120
    ],
    "debug.allowBreakpointsEverywhere": true,
    "update.mode": "none",
    "stm32-for-vscode.openOCDPath": "false",
    "stm32-for-vscode.makePath": "/usr/bin/make",
    "stm32-for-vscode.armToolchainPath": "/usr/bin",
    "security.workspace.trust.untrustedFiles": "open",
    "cmake.configureOnOpen": true,
    "terminal.integrated.automationShell.linux": "",
    "terminal.integrated.defaultProfile.linux": "fish",
    "terminal.integrated.profiles.linux": {
        "fish": {
            "path": "/usr/bin/fish",
            "args": []
        }
    },
    "git.autofetch": true
}
